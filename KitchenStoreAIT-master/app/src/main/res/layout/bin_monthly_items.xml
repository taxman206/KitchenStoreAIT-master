<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/bin_title_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:text="Bin Items"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/february_button"
        android:layout_width="94dp"
        android:layout_height="96dp"
        android:layout_marginStart="48dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="21dp"
        android:text="February"
        app:layout_constraintBottom_toTopOf="@+id/may_button"
        app:layout_constraintEnd_toStartOf="@+id/march_button"
        app:layout_constraintStart_toEndOf="@+id/january_button" />

    <Button
        android:id="@+id/january_button"
        android:layout_width="94dp"
        android:layout_height="96dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="21dp"
        android:text="January"
        app:layout_constraintBottom_toTopOf="@+id/april_button"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/march_button"
        android:layout_width="94dp"
        android:layout_height="96dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="21dp"
        android:text="March"
        app:layout_constraintBottom_toTopOf="@+id/june_button"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/june_button"
        android:layout_width="94dp"
        android:layout_height="96dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="21dp"
        android:text="JUNE"
        app:layout_constraintBottom_toTopOf="@+id/september_button"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/may_button"
        android:layout_width="94dp"
        android:layout_height="96dp"
        android:layout_marginStart="48dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="21dp"
        android:text="MAY"
        app:layout_constraintBottom_toTopOf="@+id/august_button"
        app:layout_constraintEnd_toStartOf="@+id/june_button"
        app:layout_constraintStart_toEndOf="@+id/april_button" />

    <Button
        android:id="@+id/september_button"
        android:layout_width="104dp"
        android:layout_height="88dp"
        android:text="SEPTEMBER"
        tools:layout_editor_absoluteX="294dp"
        tools:layout_editor_absoluteY="370dp" />

    <Button
        android:id="@+id/august_button"
        android:layout_width="99dp"
        android:layout_height="90dp"
        android:layout_marginStart="48dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="23dp"
        android:text="AUGUST"
        app:layout_constraintBottom_toTopOf="@+id/november_button"
        app:layout_constraintEnd_toStartOf="@+id/september_button"
        app:layout_constraintStart_toEndOf="@+id/july_button" />

    <Button
        android:id="@+id/november_button"
        android:layout_width="101dp"
        android:layout_height="93dp"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="42dp"
        android:layout_marginBottom="20dp"
        android:text="NOVEMBER"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/december_button"
        app:layout_constraintStart_toEndOf="@+id/october_button" />

    <Button
        android:id="@+id/december_button"
        android:layout_width="98dp"
        android:layout_height="94dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="17dp"
        android:text="DECEMBER"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/october_button"
        android:layout_width="94dp"
        android:layout_height="96dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="17dp"
        android:text="October"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/april_button"
        android:layout_width="94dp"
        android:layout_height="96dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="21dp"
        android:text="APRIL"
        app:layout_constraintBottom_toTopOf="@+id/july_button"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/july_button"
        android:layout_width="94dp"
        android:layout_height="96dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="24dp"
        android:text="JULY"
        app:layout_constraintBottom_toTopOf="@+id/october_button"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>